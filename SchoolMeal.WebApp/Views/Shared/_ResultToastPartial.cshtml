<div class="toast align-items-center text-bg-success border-0 bottom-0 end-0 m-4 position-absolute" style="z-index:9999;" role="alert" aria-live="assertive" aria-atomic="true">
    <div class="d-flex">
        <div class="toast-body fw-bold d-flex align-items-center ">
            <i class="bi bi-check-circle-fill fs-5 me-2"></i>
            @TempData["Result"]
        </div>
        <button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast" aria-label="Close"></button>
    </div>
</div>